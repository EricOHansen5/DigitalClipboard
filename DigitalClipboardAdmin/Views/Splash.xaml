<Window x:Class="DigitalClipboardAdmin.Views.Splash"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DigitalClipboardAdmin.Views"
        mc:Ignorable="d"
        Title="Loading" Height="200" Width="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBlock Height="21" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="0,50,0,0">Digital Clipboard is Loading</TextBlock>
        <ProgressBar Name="progBar" Margin="10,100,10,10" Minimum="0" Maximum="1" Height="30"/>
    </Grid>
</Window>
